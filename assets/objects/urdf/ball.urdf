<?xml version="1.0" ?>
<robot name="ball" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <link name="ball">
    <inertial>
      <mass value=".1"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="1e5" ixy="0" ixz="0" iyy="1e5" iyz="0" izz="1e5"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius=".01"/>
          <!--<mesh filename="meshes/ball.obj"/>-->
      </geometry>
      <material name="Grey">
        <color rgba="0.65 0.65 0.65 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius=".01"/>
        <!--<mesh filename="meshes/ball.obj"/>-->
      </geometry>
    </collision>
  </link>
</robot>
